{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:vue/recommended",
    "prettier",
    "prettier/vue"
  ],
  "parser": "vue-eslint-parser",
  "parserOptions": {
    "parser": "babel-eslint"
  },
  "plugins": ["vue", "prettier"],
  "root": true,
  "rules": {
    "no-console": 0,
    "prettier/prettier": "error",
    "semi": 0
  }
}
